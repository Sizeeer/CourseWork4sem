<Window x:Class="WPFUI.CreateTournamentDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="TournamentDashboard" Height="320.53" Width="307.263">
    <Grid>
        <Label Content="Tournament dashboard" RenderTransformOrigin="0.477,0.485" FontSize="25" Margin="12,28,-2,0" HorizontalAlignment="Left" Width="286" Height="58" VerticalAlignment="Top"/>
        <Label Content="Загрузите прошедший турнир" Margin="75,93,0,0" HorizontalAlignment="Left" Width="200" Height="28" VerticalAlignment="Top"/>
        <ComboBox Name="cbLoadExistingTournament" HorizontalAlignment="Left" Margin="57,121,0,0" VerticalAlignment="Top" Width="196"/>
        <Button Content="Загрузить" HorizontalAlignment="Left" Margin="118,169,0,0" VerticalAlignment="Top" Width="74" Click="btnLoadTournament_Click" />
        <Button Content="Создать турнир" HorizontalAlignment="Left" Margin="83,209,0,0" VerticalAlignment="Top" Width="144" Height="54" Click="btnCreateTournament_Click"/>

    </Grid>
</Window>

